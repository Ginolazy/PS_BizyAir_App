{
    "id": "PS_BizyAir_App",
    "name": "PS_BizyAir_App",
    "version": "1.1.2",
    "main": "index.html",
    "manifestVersion": 5,
    "host": {
        "app": "PS",
        "minVersion": "25.0.0"
    },
    "featureFlags": {
        "enableAlerts": true,
        "enableDeveloperTools": true,
        "enableRemoteInspection": true
    },
    "requiredPermissions": {
        "allowCodeGenerationFromStrings": true,
        "localFileSystem": "fullAccess",
        "launchProcess": {
            "schemes": [
                "http",
                "https"
            ],
            "extensions": [
                ".svg",
                ".png",
                ".jpg"
            ]
        },
        "network": {
            "domains": "all"
        },
        "clipboard": "readAndWrite",
        "webview": {
            "allow": "yes",
            "allowLocalRendering": "yes",
            "domains": "all",
            "enableMessageBridge": "localAndRemote"
        },
        "photoshop": {
            "allowOpen": true,
            "allowModify": true,
            "allowExecuteAsModal": true
        }
    },
    "entrypoints": [
        {
            "type": "panel",
            "id": "bizyairPanel",
            "main": "index.html",
            "label": {
                "default": "PS_BizyAir_App"
            },
            "minimumSize": {
                "width": 260,
                "height": 400
            },
            "maximumSize": {
                "width": 600,
                "height": 1200
            },
            "preferredDockedSize": {
                "width": 260,
                "height": 600
            },
            "preferredFloatingSize": {
                "width": 260,
                "height": 600
            },
            "icons": [
                {
                    "width": 24,
                    "height": 24,
                    "path": "icons/icon-24.png",
                    "scale": [
                        1,
                        2
                    ],
                    "theme": [
                        "darkest",
                        "dark",
                        "medium",
                        "lightest",
                        "light"
                    ]
                },
                {
                    "width": 48,
                    "height": 48,
                    "path": "icons/icon-48.png",
                    "scale": [
                        1,
                        2
                    ],
                    "theme": [
                        "darkest",
                        "dark",
                        "medium",
                        "lightest",
                        "light"
                    ]
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 24,
            "height": 24,
            "path": "icons/icon-24.png",
            "scale": [
                1,
                2
            ],
            "theme": [
                "darkest",
                "dark",
                "medium",
                "lightest",
                "light"
            ],
            "species": [
                "pluginList"
            ]
        },
        {
            "width": 48,
            "height": 48,
            "path": "icons/icon-48.png",
            "scale": [
                1,
                2
            ],
            "theme": [
                "darkest",
                "dark",
                "medium",
                "lightest",
                "light"
            ],
            "species": [
                "pluginList"
            ]
        }
    ]
}